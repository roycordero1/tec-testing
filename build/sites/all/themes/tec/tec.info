name        = TEC
description = Drupal 7 theme personalizado para TEC.
engine      = phptemplate
core        = 7.x

; REGIONS
;-----------------------------------------------
regions[content] = Content

; CSS
;-----------------------------------------------
stylesheets[all][] = css/style.css

; JS
;-----------------------------------------------
scripts[] = js/vendors/modernizr-custom.js
scripts[] = js/vendors/masonry.pkgd.min.js
scripts[] = js/behaviors.js

; PANELS
;-----------------------------------------------
plugins[panels][layouts] = panels/layouts

; BREAKPOINTS
;-----------------------------------------------
breakpoints[xs] = (min-width: 320px)
breakpoints[s]  = (min-width: 480px)
breakpoints[m]  = (min-width: 768px)
breakpoints[l]  = (min-width: 960px)
breakpoints[xl] = (min-width: 1020px)
